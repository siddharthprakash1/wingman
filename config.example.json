{
  "agents": {
    "defaults": {
      "workspace": "~/.openclaw_mine/workspace",
      "model": "ollama/kimi-k2.5:cloud",
      "max_tokens": 8192,
      "temperature": 0.7,
      "max_tool_iterations": 25
    }
  },
  "providers": {
    "kimi": {
      "api_key": "",
      "api_base": "https://api.moonshot.ai/v1",
      "model": "kimi-k2.5"
    },
    "gemini": {
      "api_key": "",
      "api_base": "https://generativelanguage.googleapis.com/v1beta"
    },
    "ollama": {
      "api_base": "http://localhost:11434",
      "model": "kimi-k2.5:cloud"
    },
    "openrouter": {
      "api_key": "",
      "api_base": "https://openrouter.ai/api/v1"
    }
  },
  "channels": {
    "telegram": {
      "enabled": false,
      "token": "",
      "allow_from": []
    },
    "discord": {
      "enabled": false,
      "token": "",
      "allow_from": []
    },
    "webchat": {
      "enabled": true,
      "port": 8080
    }
  },
  "tools": {
    "web": {
      "search": {
        "provider": "duckduckgo",
        "api_key": "",
        "max_results": 5
      }
    },
    "shell": {
      "enabled": true,
      "allowed_commands": ["*"]
    },
    "browser": {
      "enabled": true
    },
    "cron": {
      "enabled": true
    }
  },
  "gateway": {
    "port": 18789,
    "host": "127.0.0.1"
  }
}
